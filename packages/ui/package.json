  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "storybook dev -p 6006",
    "build": "tsup",
    "clean": "rm -rf dist storybook-static",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static -p 6008 --silent",
    "storybook:test": "test-storybook --maxWorkers=4 --url http://localhost:6008",
    "test": "start-server-and-test 'storybook dev -p 6006' http://localhost:6006 'test-storybook --url http://localhost:6006 --json --outputFile test-results.json'",
    "test:watch": "test-storybook --watch",
    "test:failfast": "node ./scripts/run-tests-failfast.js",
    "test:ci": "pnpm storybook:build && (concurrently -k -s first -n 'serve,test' -c 'bgBlue.bold,bgGreen.bold' 'pnpm storybook:serve' 'pnpm storybook:test')",
    "test:coverage": "pnpm storybook:build && (concurrently -k -s first -n 'serve,test' -c 'bgBlue.bold,bgGreen.bold' 'pnpm storybook:serve' 'pnpm storybook:test --coverage')",
    "lint:files": "bash -c 'eslint \"${@:-.}\" --max-warnings 0' _",
    "lint": "pnpm run lint:files .",
    "lint:fix": "bash -c 'eslint \"${@:-.}\" --max-warnings 0 --fix' _",
    "typecheck": "tsc --noEmit"
  },
